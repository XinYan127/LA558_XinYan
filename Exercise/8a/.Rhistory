str_pad(2, "left", "0") %>%
paste0("S0101_C03_0", .) %>%
set_names(cohort_names)
female_vars <- 2:19 %>%
str_pad(2, "left", "0") %>%
paste0("S0101_C05_0", .) %>%
set_names(cohort_names)

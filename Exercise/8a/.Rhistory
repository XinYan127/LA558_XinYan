male_data <- get_acs(
geography = "county",
variables = male_vars,
state = "MI",
county = "Washtenaw",
year = 2020
) %>%
mutate(sex = "Male",
estimate = estimate * -1)
female_data <- get_acs(
geography = "county",
variables = female_vars,
state = "MI",
county = "Washtenaw",
year = 2020
) %>%
mutate(sex = "Female")
pyramid_data <- bind_rows(male_data, female_data) %>%
mutate(variable = factor(variable, levels = cohort_names)) #<<
View(pyramid_data)
